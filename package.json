{
  "name": "tampermonkey",
  "version": "0.0.21",
  "description": "custom scripts for tampermonkey",
  "main": "index.js",
  "repository": "git@github.com:flinhong/tampermonkey.git",
  "author": "Frank Lin <honglin@duck.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build:scss:sites": "sass ./src/sites/:./public/styles/ --no-source-map --style=expanded",
    "build:scss": "sass ./src/browserStyle.scss ./public/styles.min.css --no-source-map --style=compressed",
    "build": "yarn run build:scss && node index.js",
    "public": "npx auto-version && yarn run build"
  },
  "dependencies": {
    "js-beautify": "^1.14.9",
    "sass": "^1.66.1"
  },
  "devDependencies": {
    "auto-version-js": "^0.3.10",
    "prettier": "^3.2.4"
  }
}
